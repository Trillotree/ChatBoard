<script>
  // Function to handle sticker selection
  function selectSticker(sticker) {
    const username = document.getElementById("username").value;

    if (!username) {
      alert("Please provide a username.");
      return;
    }

    // Create a new message element with the selected sticker
    const messageElement = document.createElement("div");
    messageElement.classList.add("message");
    messageElement.innerHTML =
      "<strong>" +
      username +
      "</strong>: <img src='" +
      sticker.src +
      "' alt='Sticker' class='content' style='width: 80px; height: 80px;' />";

    // Append the new message to the chat board
    const chatBoardInner = document.getElementById("chat-board-inner");
    chatBoardInner.appendChild(messageElement);

    // Scroll to the newest message
    chatBoardInner.scrollTop = chatBoardInner.scrollHeight;

    // Hide the sticker dropdown
    document.querySelector(".sticker-dropdown").style.display = "none";
  }

  // Toggle the visibility of the sticker picker dropdown
  document
    .querySelector(".emoji-button")
    .addEventListener("click", function () {
      const dropdown = document.querySelector(".sticker-dropdown");
      dropdown.style.display =
        dropdown.style.display === "none" ? "block" : "none";
    });

  // Add event listeners to each folder image to display the relevant stickers
  document.querySelectorAll(".folder img").forEach((folder) => {
    folder.addEventListener("click", function () {
      const folderId = this.getAttribute("data-folder");

      // Hide all sticker containers
      document.querySelectorAll(".sticker-container").forEach((container) => {
        container.style.display = "none";
      });

      // Show the selected folder's sticker container
      const selectedFolder = document.getElementById(folderId);
      if (selectedFolder) {
        selectedFolder.style.display = "grid";
      }
    });
  });

  // Close dropdown when clicking outside
  document.addEventListener("click", function (event) {
    const dropdown = document.querySelector(".sticker-dropdown");
    const emojiButton = document.querySelector(".emoji-button");
    if (
      !emojiButton.contains(event.target) &&
      !dropdown.contains(event.target)
    ) {
      dropdown.style.display = "none";
    }
  });
</script>

<!-- Section 2 -->
<!-- Sticker Button Emojis Below - End -->
<!-- Sticker DropDown Emojis Below - End -->


</div>

