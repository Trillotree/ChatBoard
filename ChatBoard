      <script>
        // Function to handle emoji selection
        function selectEmoji(emoji) {
          const username = document.getElementById("username").value;
  
          if (!username) {
            alert("Please provide a username.");
            return;
          }
  
  
          // Append the new message to the chat board
          const chatBoardInner = document.getElementById("chat-board-inner");
          chatBoardInner.appendChild(messageElement);
  
          // Scroll to the newest message
          chatBoardInner.scrollTop = chatBoardInner.scrollHeight;
  
          // Hide the emoji dropdown
          document.querySelector(".emoji-dropdown").style.display = "none";
        }
  
        // Toggle the visibility of the emoji picker dropdown
        function toggleEmojiDropdown() {
          const dropdown = document.querySelector(".emoji-dropdown");
          dropdown.style.display =
            dropdown.style.display === "none" ? "block" : "none";
        }
  
        // Add event listeners to each folder image to display the relevant emojis
        document.querySelectorAll(".folder img").forEach((folder) => {
          folder.addEventListener("click", function () {
            const folderId = this.getAttribute("data-folder");

 // Hide all emoji containers
              document.querySelectorAll(".emoji-container").forEach((container) => {
                container.style.display = "none";
              });

              // Show the selected folder's emoji container
              const selectedFolder = document.getElementById(folderId);
              if (selectedFolder) {
                selectedFolder.style.display = "grid";
              }
            });
          });

          // Close dropdown when clicking outside
          document.addEventListener("click", function (event) {
            const dropdown = document.querySelector(".emoji-dropdown");
            const emojiButton = document.querySelector("#emoji-btn");
            if (!emojiButton.contains(event.target) && !dropdown.contains(event.target)) {
              dropdown.style.display = "none";
            }
          });
        </script>
      </body>
    </html>


<!-- Section 1 - END -->
<!-- Emoji Sticker - Emoji ButtonEmojis Below - End -->
<!-- Emoji Sticker DropDown Emojis Below - End -->


</Div>

