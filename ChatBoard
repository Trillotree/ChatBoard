// Function to buy coins
function buyCoins(url) {
window.open(url, "_blank");
}

// Function to deduct coins
function deductCoins(amount) {
const currentCoins = getCookie("coins") || 0;
if (currentCoins < amount) {
return false;
}
setCookie("coins", currentCoins - amount, 30);
updateCoinCounter();
return true;
}

// Function to update coin counter
function updateCoinCounter() {
const currentCoins = getCookie("coins") || 0;
document.getElementById("coin-counter").textContent = currentCoins;
}

// Function to set cookie
function setCookie(name, value, days) {
const d = new Date();
d.setTime(d.getTime() + days * 24 * 60 * 60 * 1000);
const expires = "expires=" + d.toUTCString();
document.cookie = name + "=" + value + ";" + expires + ";path=/";
}

// Function to get cookie
function getCookie(name) {
const cname = name + "=";
const decodedCookie = decodeURIComponent(document.cookie);
const ca = decodedCookie.split(";");
for (let i = 0; i < ca.length; i++) {
let c = ca[i];
while (c.charAt(0) == " ") {
 c = c.substring(1);
}
if (c.indexOf(cname) == 0) {
 return c.substring(cname.length, c.length);
}
}
return "";
}

// Function to show popup for free coin
function showPopup() {
document.getElementById("coin-popup").style.display = "block";
}

// Function to close popup for free coin
function closePopup() {
document.getElementById("coin-popup").style.display = "none";
}

// 1 free coin after 30 days
// Function to reward free coin after 30 consecutive days
function rewardFreeCoin() {
const lastVisitDate = getCookie("lastVisitDate");
const currentDate = new Date();
const thirtyDaysInMillis = 30 * 24 * 60 * 60 * 1000; // 30 days in milliseconds

if (!lastVisitDate) {
setCookie("lastVisitDate", new Date().toUTCString(), 30);
return;
}

const lastVisit = new Date(lastVisitDate);
const daysDifference = Math.floor((currentDate - lastVisit) / (24 * 60 * 60 * 1000));

if (daysDifference === 1) {
const visitCount = parseInt(getCookie("visitCount") || "0") + 1;
setCookie("visitCount", visitCount, 30);

if (visitCount >= 30) {
 const currentCoins = getCookie("coins") || 0;
 setCookie("coins", parseInt(currentCoins) + 1, 30);
 setCookie("visitCount", "0", 30); // Reset visit count after awarding the coin
 updateCoinCounter();
 showPopup();
}
} else if (daysDifference > 1) {
setCookie("visitCount", "1", 30); // Reset visit count if a day is missed
}

setCookie("lastVisitDate", new Date().toUTCString(), 30);
}

// Initial setup
document.addEventListener("DOMContentLoaded", function () {
updateCoinCounter();
rewardFreeCoin();
});

// 1 free coin after 30 days

// Close dropdown when clicking outside
document.addEventListener("click", function (event) {
const emojiDropdown = document.querySelector(".emoji-dropdown");
const stickerDropdown = document.querySelector(".sticker-dropdown");
const giftDropdown = document.querySelector(".gift-dropdown");
const emojiButton = document.querySelector("#emoji-btn");
const stickerButton = document.querySelector("#sticker-btn");
const giftButton = document.querySelector("#gift-btn");
if (
!emojiButton.contains(event.target) &&
!emojiDropdown.contains(event.target)
) {
emojiDropdown.style.display = "none";
}
if (
!stickerButton.contains(event.target) &&
!stickerDropdown.contains(event.target)
) {
stickerDropdown.style.display = "none";
}
if (
!giftButton.contains(event.target) &&
!giftDropdown.contains(event.target)
) {
giftDropdown.style.display = "none";
}
});

// Keep from Spamming - Anti spam messages
// Function to handle message sending
function sendMessage() {
const username = document.getElementById("username").value;
let message = document.getElementById("message-input").value;

if (!username) {
alert("Please add a username to send a message.");
return;
}

// Check for spamming
if (isSpamming(message)) {
alert("You are sending too many similar messages. Please wait for 1 minute.");
return;
}

// No foul language filter
const foulWords = ["ass","bastard","bitch","crap","cunt","damn","dick","douche","faggot","fuck"," motherfucker","piss","prick","pussy","shit","slut","twat","whore","cock"]; // Add more foul words as needed
const regex = new RegExp(foulWords.join("|"), "gi");
message = message.replace(regex, (matched) => "*".repeat(matched.length));

// Check for admin commands
if (adminUsernames.includes(username.toLowerCase())) {
if (message.startsWith("@")) {
 const [command, targetUsername, ...args] = message.split(" ");
 handleAdminCommand(command, targetUsername, args);
 return;
}
}

// Create a new message element
const messageElement = document.createElement("div");
messageElement.classList.add("message");
messageElement.innerHTML =
"<strong>" +
username +
"</strong>: <p class='content'>" +
message +
"</p>";

// Append the new message to the chat board
const chatBoardInner = document.getElementById("chat-board-inner");
chatBoardInner.appendChild(messageElement);

// Clear the message input
document.getElementById("message-input").value = "";

// Scroll to the newest message
chatBoardInner.scrollTop = chatBoardInner.scrollHeight;

// Record the message
recordMessage(message);
}

const messageHistory = {};
const cooldownUsers = new Set();
function isSpamming(message) {
  const username = document.getElementById("username").value;
  if (cooldownUsers.has(username)) {
    return true;
  }

  if (!messageHistory[username]) {
    messageHistory[username] = [];
  }

  const now = Date.now();
  messageHistory[username] = messageHistory[username].filter(
    (entry) => now - entry.time < 1 * 60 * 1000
  );

  messageHistory[username].push({ message, time: now });

  const sameMessageCount = messageHistory[username].filter(
    (entry) => entry.message === message
  ).length;

  if (sameMessageCount >= 4) {
    cooldownUsers.add(username);
    setTimeout(() => cooldownUsers.delete(username), 1 * 60 * 1000);
    return true;
  }

  return false;
}

function recordMessage(message) {
  const username = document.getElementById("username").value;
  if (!messageHistory[username]) {
    messageHistory[username] = [];
  }

  messageHistory[username].push({ message, time: Date.now() });
}

// Admin-related functions and variables
const adminUsernames = [
  "fatty patty", "fattypatty", "fattypattyworkouts", "fatty patty workouts", "fatty patty workout", "fattypattyworkout",
  "diaryofafatlady", "diary of a fat lady","pretty patty workouts",  "prettypattyworkouts", "pretty patty workout", 
  "prettypattyworkout","pretty patty fitness","prettypattyfitness", "pretty patty", "prettypatty",
].map(username => username.toLowerCase());

// Function to check the username (can be customized for validation)
function checkUsername() {
  const username = document.getElementById("username").value.toLowerCase(); // Convert to lowercase
  if (adminUsernames.includes(username)) {
    const password = prompt("Enter admin password:");
    if (!password || !validatePassword(password)) {
      alert("Invalid password. You cannot use this username.");
      document.getElementById("username").value = "";
    }
  }

  if (!username) {
    document.getElementById("error-message").textContent =
      "Username is required!";
  } else {
    document.getElementById("error-message").textContent = "";
  }
}

const adminPasswordHash = "$2y$04$jh3DIFlG5KBfdtgnnMOKNuZlUA7xYx48xp.iSKYt5OzopoK1KOXQa";
const bcrypt = dcodeIO.bcrypt;

function handleAdminCommand(command) {
  const parts = command.split(' ');
  const targetUsername = parts[0].substring(1); // Remove the '@' symbol
  const action = parts[1];
  const args = parts.slice(2);

  if (action === "Coin") {
    const amount = parseInt(args[0], 10);
    if (amount === 1 || amount === 2) {
      awardCoins(targetUsername, amount);
    } else {
      alert("Invalid coin amount. Use 1 or 2.");
    }
  } else if (action === "ban") {
    const minutes = parseInt(args[0], 10);
    if (minutes > 0 && minutes <= 60) {
      banUser(targetUsername, minutes);
    } else {
      alert("Invalid ban duration. Use 1-60 minutes.");
    }
  }
}

// Example usage
handleAdminCommand('@someuser ban 30');

function awardCoins(username, amount) {
  const currentCoins = getCookie("coins") || 0;
  setCookie("coins", parseInt(currentCoins) + amount, 30);
  updateCoinCounter();
  alert(`Awarded ${amount} coins to ${username}.`);
}

function banUser(username, minutes) {
  cooldownUsers.add(username);
  setTimeout(() => cooldownUsers.delete(username), minutes * 60 * 1000);
  alert(`Banned ${username} for ${minutes} minutes.`);
}

function checkMultipleAdmins() {
  const currentAdmin = document.getElementById("username").value;
  const loggedInAdmins = adminUsernames.filter(username => username !== currentAdmin && username in messageHistory);
  if (loggedInAdmins.length > 0) {
    const otherAdmin = loggedInAdmins[0];
    const otherAdminIP = getCookie(otherAdmin + "_IP");
    const kick = confirm(`Another admin (${otherAdmin}) is logged in from IP ${otherAdminIP}. Do you want to kick them?`);
    if (kick) {
      cooldownUsers.add(otherAdmin);
      alert(`Kicked ${otherAdmin} from the chat.`);
    }
  }
}

//SUPER BANNED People.
function permanentlyBanIP(ip) {
  setCookie(ip + "_banned", "true", 365 * 10); // Ban for 10 years
}

const permanentlyBannedIPs = ["192.0.2.1", "192.0.2.3", "192.0.2.4"];
permanentlyBannedIPs.forEach(ip => permanentlyBanIP(ip));

// Function to check if an IP is permanently banned
function isIPBanned(ip) {
  return getCookie(ip + "_banned") === "true";
}

// Check for the user's IP and ban status on page load
document.addEventListener("DOMContentLoaded", function () {
  const userIP = "192.0.2.1"; // Replace with actual method to get user's IP
  if (isIPBanned(userIP)) {
    alert("Your IP is permanently banned from using this chat.");
    document.body.innerHTML = ""; // Hide chat if IP is banned
  } else {
    updateCoinCounter();
    rewardFreeCoin();
    checkMultipleAdmins();
  }
});
      </script>
  </body>
</html>
